library ieee;
use ieee.std_logic_1164.all;
library work;
use work.Gates.all;

entity 2x1MUX is
port (I0, I1,S: in std_logic; Y: out std_logic);
end entity 2x1MUX;

architecture Struct of 2x1MUX is
signal A,B,C : std_logic;


begin
  --component instances
  
  NOT1 : INVERTER port map(S,A)
  AND1 : AND_2 port map (I0,A,B);
  AND2 : AND_2 port map (I1,A,C);
  OR1  : OR_2 port map (B,C,Y);
  
  end Struct;